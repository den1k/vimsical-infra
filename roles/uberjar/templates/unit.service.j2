[Unit]
Description={{ service_description }}

[Service]
{% for k, v in service_env.iteritems() %}
Environment="{{ k }}={{ v }}"
{% endfor %}
ExecStart=/usr/bin/java {{ jvm_options }} -jar {{ jar_dir }}/{{ jar_name }}
Type={{ service_type }}
User={{ service_user }}
Restart={{ service_restart }}
RestartSec=30
[Install]
WantedBy={{ service_target }}
